block qq:if="{{itemcon.state==2&&surplusTime>0}}"><view class="rwBtn" data-time="{{surplusTime}}" data-event-opts="{{[['tap',[['goonTask',['$event']]]]]}}" bindtap="__e"><view class="taskgoon">任务进行中</view></view></block><block qq:if="{{itemcon.state==2&&surplusTime==0}}"><view class="rwBtn"><view class="taskover">任务已超时</view></view></block><block qq:if="{{itemcon.state==3}}"><view class="rwBtn"><view class="taskover">任务已完成</view></view></block><block qq:if="{{itemcon.state==4}}"><view class="rwBtn"><view class="taskover">任务已失败</view></view></block><view class="timebox"><view class="rwTime">预计时间：<text>{{itemcon.duration+"分钟"}}</text></view><block qq:if="{{itemcon.state==3}}"><view class="rwTime">实际完成时间<text>{{realtime}}</text></view></block><block qq:if="{{itemcon.state==4}}"><view class="rwTime">实际完成时间<text>{{realtime}}</text></view></block><block qq:if="{{itemcon.state==2}}"><view class="rwTime">开始时间:<text>{{shortTime}}</text></view></block></view></view><block qq:if="{{itemcon.completionSwitch&&itemcon.state!==4&&!itemcon.completionCheck}}"><view class="rwOther"><view class="title">完成质量:</view><radio-group data-event-opts="{{[['change',[['radioChange',['$event']]]]]}}" class="radiogroup" bindchange="__e"><block qq:for="{{items}}" qq:for-item="item" qq:for-index="index" qq:key="value"><label><radio style="transform:scale(0.7);" color="#ffce00" value="{{item.value}}" checked="{{index===current}}"></radio><view>{{item.name}}</view></label></block></radio-group><block qq:if="{{clickable}}"><view data-event-opts="{{[['tap',[['zhiliangEvent',['$event']]]]]}}" class="{{[itemcon.flag==1?'zhiliangBtn':'zhiliangBtn1']}}" bindtap="__e">确定</view></block><block qq:if="{{clickable==false}}"><view class="{{[itemcon.flag==1?'zhiliangBtn':'zhiliangBtn1']}}">确定</view></block></view></block></view>